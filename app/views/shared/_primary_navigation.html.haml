.navbar.navbar-inverse.navbar-fixed-top{ role: "navigation" }
  .container
    .navbar-header
      %button.navbar-toggle{ type: "button", "data-toggle" =>"collapse", "data-target" => ".navbar-collapse" }
        %span.sr-only
          Toggle navigation
        .icon-bar
        .icon-bar
        .icon-bar

      %a.navbar-brand{ href: root_path }
        Startup Victoria

    .navbar-collapse.collapse
      %ul.nav.navbar-nav.navbar-set-active
        %li.active
          %a{ href: root_path }
            Home
        %li
          %a{ href: coupons_path }
            Coupons
        %li
          %a{ href: memberships_path }
            Membership
        %li
          %a{ href: events_path }
            Events
        %li
          %a{ href: new_contact_path }
            Contact
        %li
          %a{ href: about_us_path }
            About

      - if !content_for? :spartan_header_footer
        = render "shared/primary_navigation_right"
